{
  "name": "xmlrpc-serialization",
  "version": "2.0.0",
  "description": "XmlRPC decode and stringify serialization functions",
  "exports": {
    "./decode": {
      "node": "./lib/decode_node.js",
      "import": "./lib/decode.js"
    },
    "./stringify": {
      "node": "./lib/stringify_node.js",
      "import": "./lib/stringify.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piranna/XmlRPC-serialization.git"
  },
  "keywords": [
    "XmlRPC",
    "DOMparser",
    "XML",
    "RPC",
    "XMLserializer"
  ],
  "author": "Jesús Leganés-Combarro 'piranna' <piranna@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/piranna/XmlRPC-serialization/issues"
  },
  "homepage": "https://github.com/piranna/XmlRPC-serialization#readme",
  "devDependencies": {
    "jest-cli": "^27.2.4",
    "jest-node-exports-resolver": "github:Mafalda-SFU/jest-node-exports-resolver",
    "prettier": "^2.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "resolver": "jest-node-exports-resolver"
  }
}
